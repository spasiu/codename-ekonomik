<!DOCTYPE html>
<html>
  <head>
    <link type="text/css" href="css/style.css" rel="stylesheet">
  </head>

  <body>
    <% include header %>

    <div class="main">
      <#% var userItems = [{description: "words", image_link: ""}] %>
      <% userItems.forEach(function(item) { %>
        <img src='<%= item.image_link %>'>
        <p><%= item.description %></p>
      <% }); %>
    </div>

    <% include footer %>

  </body>
</html>