<!DOCTYPE html>
<html>
  <head>
    <link type="text/css" href="css/style.css" rel="stylesheet">
    <link type="text/css" href="css/list.css" rel="stylesheet">
  </head>

  <body>
    <% include header %>
    <br>
    <div class="main">
      <br>
      <h2 class="owner"><%= user %>'s Items</h2>
      <% include switch_views %>
      <% userItems.forEach(function(item) { %>
        <img src='<%= item.image_link %>'>
        <div class="about">
          <p><h3><%= item.name %></h3></p>
          <p><%= item.description %></p>
        </div>
        <hr>
      <% }); %>
      <% include new_item %>
    </div>

    <% include footer %>

  </body>
</html>
